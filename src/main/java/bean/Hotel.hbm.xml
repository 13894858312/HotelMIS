<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bean.Hotel" table="Hotel" schema="HotelMIS">
        <id name="hid">
            <column name="hid" sql-type="int(11)"/>
        </id>
        <property name="pwd">
            <column name="pwd" sql-type="text"/>
        </property>
        <property name="createDate">
            <column name="create_date" sql-type="date"/>
        </property>
        <property name="hname">
            <column name="hname" sql-type="text"/>
        </property>
        <property name="city">
            <column name="city" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>